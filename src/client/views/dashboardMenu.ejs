<div id="dashboard-menu">
    <div id="head">
        <i class="fas fa-angle-double-right"></i>
        <h3>Dashboard</h3>
    </div>
    <div class="row" onclick="document.location='/profile'">
        <i class="fas fa-id-badge"></i>
        <h4>Profile</h4>
    </div>
    <div class="row" onclick="document.location='/dashboard_appointments'">
        <i class="fas fa-calendar-check"></i>
        <h4>Appointments</h4>
    </div>
    <% if(session.isDoctor) { %>
        <div class="row" onclick="document.location='/add-schedule'">
            <i class="fas fa-calendar-check"></i>
            <h4>Add Schedule</h4>
        </div>
    <% } %>
    <div class="row">
        <i class="fas fa-file-medical-alt"></i>
        <h4>Medical Reports</h4>
    </div>
    <div class="row" onclick="document.location='/settings'">
        <i class="fas fa-cog"></i>
        <h4>Settings</h4>
    </div>
</div>